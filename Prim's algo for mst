const int maxn = 2e5 + 5;
const int MOD = 1e9 + 7;
const int MM = 998244353;

const int N = 1e5 + 1;



void solve()
{
	ll n, m;
	cin >> n >> m;
	vector<pair<ll, ll>> g[n];
	vll parent(n, -1), key(n, INT_MAX);
	key[0] = 0;
	vector<bool> mst(n, false);
	while (m--)
	{
		ll u, v, w; cin >> u >> v >> w;
		g[u].pb({v, w});
		g[v].pb({u, w});

	}

	priority_queue<pair<ll, ll>, vector<pair<ll, ll>>, greater<pair<ll, ll>>> p;
	p.push({0, 0});
	while (!p.empty())
	{
		ll u = p.top().ss;
		p.pop();
		mst[u] = true;
		for (auto x : g[u])
		{
			ll w = x.ss;
			ll v = x.ff;
			if (key[v] > w and mst[v] == false)
			{
				key[v] = w;
				parent[v] = u;
				p.push({key[v], v});

			}
		}


	}
	for (int i = 1; i < n; ++i)
	{
		cout << parent[i] << "->" << i << '\n';
	}


}





int32_t main()
{

	FIO;
	a_k_s();
	auto start1 = high_resolution_clock::now();


	ll t = 1;


	//cin >> t;

	while (t--)
	{
		solve();
	}


	auto stop1 = high_resolution_clock::now();
	auto duration = duration_cast<microseconds>(stop1 - start1);
#ifndef ONLINE_JUDGE
	cerr << "Time: " << duration . count() / 1000 << endl;
#endif


}
